{
  "name": "demo-webpack",
  "version": "1.0.0",
  "description": "demo for webpack@5",
  "main": "main.js",
  "scripts": {
    "start": "webpack-dev-server --mode=development --env dev --node-env development",
    "build": "webpack --mode=development --node-env production",
    "build:prod": "webpack --mode=production --node-env production",
    "prepare": "husky install",
    "release": "standard-version",
    "test": "echo \"Error: no test specified\" && exit 1",
    "commit": "git pull && git add -A && git-cz && npm run push",
    "push": "git push & git push github master"
  },
  "author": {
    "name": "Franky_fwz",
    "url": "https://github.com/FC57/demo-webpack5.git"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "commitizen": "^4.3.0",
    "css-loader": "6.11.0",
    "cz-git": "^1.9.4",
    "dotenv": "^16.4.5",
    "html-webpack-plugin": "^5.6.0",
    "husky": "8.0.1",
    "loader-utils": "^3.3.1",
    "mini-css-extract-plugin": "^2.9.1",
    "postcss": "^8.4.41",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.0.2",
    "standard-version": "^9.5.0",
    "style-loader": "^4.0.0",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "core-js": "^3.38.0",
    "path-browserify": "^1.0.1",
    "regenerator-runtime": "^0.14.1"
  },
  "browserslist": [
    "last 3 version",
    ">1%"
  ],
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
