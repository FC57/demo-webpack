{
  "name": "demo-webpack",
  "version": "1.0.1",
  "description": "demo for webpack@5",
  "scripts": {
    "start": "webpack-dev-server --mode=development --node-env development",
    "build": "webpack --mode=development --node-env production",
    "build:prod": "webpack --mode=production --node-env production",
    "prepare": "husky install",
    "release": "standard-version",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:lint-staged": "lint-staged",
    "cz": "git-cz",
    "commit": "git pull && git add -A && git-cz && npm run push",
    "commit:tags": "git pull && git add -A && git-cz && npm run push:tags",
    "push": "git push & git push github master",
    "push:tags": "git push origin master --tags & git push github master --tags"
  },
  "author": {
    "name": "Franky_fwz",
    "url": "https://github.com/FC57/demo-webpack5.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-typescript": "^7.24.7",
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@types/node": "^22.5.3",
    "@types/webpack": "^5.28.5",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.3.0",
    "css-loader": "6.11.0",
    "cz-git": "^1.9.4",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "html-webpack-plugin": "^5.6.0",
    "husky": "8.0.1",
    "lint-staged": "^15.2.7",
    "loader-utils": "^3.3.1",
    "mini-css-extract-plugin": "^2.9.1",
    "postcss": "^8.4.41",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.0.2",
    "prettier": "^3.3.2",
    "standard-version": "^9.5.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2",
    "webpack": "^5.93.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "core-js": "^3.38.0",
    "path-browserify": "^1.0.1",
    "regenerator-runtime": "^0.14.1"
  },
  "browserslist": [
    "last 3 version",
    ">1%"
  ],
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
